{
  "name": "Py.Task Helper",
  "short_name": "Py.task",
  "author": "Sinasey & Avefablo",
  "homepage_url": "http://pytask.info/",
  "description": "Cтатистика python.task и Perltask",
  "version": "0.4.5",
  "content_scripts": [
    {
      "matches": [
        "http://anytask.urgu.org/course/*"
